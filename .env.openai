# OpenAI Configuration
MIMIR_LLM_PROVIDER=openai
MIMIR_LLM_API=https://api.openai.com/v1
MIMIR_LLM_API_PATH=/chat/completions
MIMIR_LLM_API_MODELS_PATH=/models
MIMIR_LLM_API_KEY=dummy-key-for-proxy

# Enable embeddings with OpenAI
MIMIR_EMBEDDINGS_ENABLED=true
MIMIR_EMBEDDINGS_PROVIDER=openai
MIMIR_EMBEDDINGS_MODEL=text-embedding-3-small

# Security (keep your existing settings)
MIMIR_ENABLE_SECURITY=true
MIMIR_ENABLE_RBAC=true
MIMIR_DEV_USERNAME=testuser
MIMIR_DEV_PASSWORD=testpass123
